{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ToolkitRegistry",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z0-9_]+$": {
      "type": "object",
      "properties": {
        "path": {
          "type": "string"
        },
        "enabled": {
          "type": "boolean"
        },
        "schema": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "required": [
        "path",
        "enabled"
      ]
    }
  },
  "additionalProperties": false
}